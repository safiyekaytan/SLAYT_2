        <script>
            var N,T,elde;
            N=5;
            var gecici;
            var A=[1,2,3,4,5];
            var B=[9,0,0,3,7];
            var C=[];
            elde=0;
            var i;
            for(i=N-1; i>-1; i--)
            {
                gecici = A[i] + B[i] +elde;
                C[i+1] = gecici %10;
                elde = gecici/10;
            }
            C[0]=elde;
            for(i=0; i<N+1; i++)
            {C[i]=Math.floor(C[i]);
                console.log(""+C[i]);
            }
        </script>
